@model dynamic

@{
    ViewData["Title"] = "Analytics";
}

<h2>Analytics</h2>

@if (ViewBag.SalesData != null)
{
    <div class="row">
        <div class="col-md-6">
            <h4>Sales Data</h4>
            <table class="table">
                <thead>
                    <tr>
                        <th>Date</th>
                        <th>Total Sales</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.SalesData)
                    {
                        <tr>
                            <td>@item.Date</td>
                            <td>@item.TotalSales</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
}
else
{
    <p>No sales data available.</p>
}